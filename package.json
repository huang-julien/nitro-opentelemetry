{
  "name": "nitro-opentelemetry",
  "version": "0.8.0",
  "description": "Opentelemetry module for the server framework nitro. Compatible with Nuxt.",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "keywords": [
    "nitro",
    "opentelemetry",
    "nuxt",
    "nuxt-module",
    "nitro module"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./runtime/*": {
      "types": "./dist/runtime/*.d.ts",
      "import": "./dist/runtime/*",
      "require": "./dist/runtime/*"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "pnpm --filter=playground dev",
    "dev:build": "pnpm --filter=playground build",
    "dev:prepare": "pnpm build --stub && pnpm nitropack prepare && pnpm nitro prepare ./playground",
    "lint": "eslint .",
    "lint:fix": "automd && eslint . --fix",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@azure/monitor-opentelemetry-exporter": "1.0.0-beta.24",
    "@baselime/node-opentelemetry": "^0.5.8",
    "@microlabs/otel-cf-workers": "1.0.0-rc.49",
    "@nuxt/schema": "^3.14.1592",
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "^3.0.9",
    "@vitest/ui": "^3.0.9",
    "automd": "^0.3.8",
    "changelogen": "^0.5.7",
    "eslint": "^9.11.0",
    "eslint-config-unjs": "^0.3.2",
    "h3": "^1.15.1",
    "jiti": "^1.21.6",
    "nitro-opentelemetry": "link:./",
    "nitro-test-utils": "^0.9.0",
    "nitropack": "^2.11.1",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0",
    "vitest": "^3.0.9"
  },
  "packageManager": "pnpm@9.15.4",
  "dependencies": {
    "@azure/monitor-opentelemetry": "1.8.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.54.0",
    "@opentelemetry/instrumentation": "0.56.0",
    "@opentelemetry/instrumentation-http": "0.56.0",
    "@opentelemetry/instrumentation-undici": "0.9.0",
    "@opentelemetry/resources": "1.29.0",
    "@opentelemetry/sdk-metrics": "1.29.0",
    "@opentelemetry/sdk-node": "0.56.0",
    "@opentelemetry/sdk-trace-node": "^1.26.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "estree-walker": "^3.0.3",
    "exsolve": "^1.0.4",
    "magic-string": "^0.30.12",
    "mlly": "^1.7.4",
    "pathe": "^2.0.3",
    "rollup": "^4.24.0"
  },
  "peerDependencies": {
    "@baselime/node-opentelemetry": "^0.5.8",
    "@microlabs/otel-cf-workers": "1.0.0-rc.49"
  }
}
