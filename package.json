{
  "name": "nitro-opentelemetry",
  "version": "0.8.1",
  "description": "Opentelemetry module for the server framework nitro. Compatible with Nuxt.",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "keywords": [
    "nitro",
    "opentelemetry",
    "nuxt",
    "nuxt-module",
    "nitro module"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./runtime/*": {
      "types": "./dist/runtime/*.d.ts",
      "import": "./dist/runtime/*",
      "require": "./dist/runtime/*"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "pnpm --filter=playground dev",
    "dev:build": "pnpm --filter=playground build",
    "dev:prepare": "pnpm build --stub && pnpm nitropack prepare && pnpm nitro prepare ./playground",
    "lint": "eslint .",
    "lint:fix": "automd && eslint . --fix",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@azure/monitor-opentelemetry-exporter": "1.0.0-beta.32",
    "@microlabs/otel-cf-workers": "1.0.0-rc.52",
    "@nuxt/schema": "^3.17.5",
    "@types/node": "^22.15.31",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "automd": "^0.4.0",
    "changelogen": "^0.6.1",
    "eslint": "^9.28.0",
    "eslint-config-unjs": "^0.4.2",
    "h3": "^1.15.3",
    "jiti": "^2.4.2",
    "nitro-opentelemetry": "link:./",
    "nitro-test-utils": "^0.9.2",
    "nitropack": "^2.11.12",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.2.3"
  },
  "packageManager": "pnpm@10.12.1",
  "dependencies": {
    "@azure/monitor-opentelemetry": "1.11.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.60.1",
    "@opentelemetry/instrumentation": "0.202.0",
    "@opentelemetry/instrumentation-http": "0.202.0",
    "@opentelemetry/instrumentation-undici": "0.13.1",
    "@opentelemetry/resources": "1.30.1",
    "@opentelemetry/sdk-metrics": "1.30.1",
    "@opentelemetry/sdk-node": "0.202.0",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "@opentelemetry/semantic-conventions": "^1.34.0",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "estree-walker": "^3.0.3",
    "exsolve": "^1.0.5",
    "magic-string": "^0.30.17",
    "mlly": "^1.7.4",
    "pathe": "^2.0.3",
    "rollup": "^4.43.0"
  },
  "peerDependencies": {
    "@microlabs/otel-cf-workers": "1.0.0-rc.52"
  }
}
