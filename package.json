{
  "name": "nitro-opentelemetry",
  "version": "0.10.1",
  "description": "Opentelemetry module for the server framework nitro. Compatible with Nuxt.",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "keywords": [
    "nitro",
    "opentelemetry",
    "nuxt",
    "nuxt-module",
    "nitro module"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./runtime/*": {
      "types": "./dist/runtime/*.d.ts",
      "import": "./dist/runtime/*",
      "require": "./dist/runtime/*"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "pnpm --filter=playground dev",
    "dev:build": "pnpm --filter=playground build",
    "dev:prepare": "pnpm build --stub && pnpm nitropack prepare && pnpm nitro prepare ./playground",
    "lint": "eslint .",
    "lint:fix": "automd && eslint . --fix",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@azure/monitor-opentelemetry-exporter": "1.0.0-beta.32",
    "@microlabs/otel-cf-workers": "1.0.0-rc.52",
    "@nuxt/schema": "^4.2.1",
    "@types/node": "^22.19.0",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "automd": "^0.4.2",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.1",
    "eslint-config-unjs": "^0.5.0",
    "h3": "^1.15.4",
    "jiti": "^2.6.1",
    "nitro-opentelemetry": "link:./",
    "nitro-test-utils": "^0.10.1",
    "nitropack": "^2.12.9",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.8"
  },
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "@azure/monitor-opentelemetry": "1.14.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@opentelemetry/instrumentation-http": "^0.208.0",
    "@opentelemetry/instrumentation-undici": "^0.19.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "estree-walker": "^3.0.3",
    "exsolve": "^1.0.7",
    "magic-string": "^0.30.21",
    "mlly": "^1.8.0",
    "pathe": "^2.0.3",
    "rollup": "^4.53.1"
  },
  "peerDependencies": {
    "@microlabs/otel-cf-workers": "1.0.0-rc.52"
  }
}
