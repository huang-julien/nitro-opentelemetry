{
  "name": "nitro-opentelemetry",
  "version": "0.0.0",
  "description": "",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "pnpm --filter=playground dev",
    "dev:prepare": "pnpm build --stub && pnpm nitropack prepare && pnpm nitro prepare ./playground",
    "lint": "eslint .",
    "lint:fix": "automd && eslint . --fix && biome check --apply .",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@azure/monitor-opentelemetry-exporter": "1.0.0-beta.24",
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.5.3",
    "automd": "^0.3.7",
    "changelogen": "^0.5.5",
    "eslint": "^9.1.1",
    "eslint-config-unjs": "^0.3.0-rc.7",
    "jiti": "^1.21.0",
    "nitro-test-utils": "^0.6.0",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.5.3"
  },
  "packageManager": "pnpm@9.0.6",
  "dependencies": {
    "@azure/monitor-opentelemetry": "^1.6.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.48.0",
    "@opentelemetry/resources": "^1.25.1",
    "@opentelemetry/sdk-metrics": "^1.25.1",
    "@opentelemetry/sdk-node": "^0.52.1",
    "@opentelemetry/sdk-trace-node": "^1.25.1",
    "@opentelemetry/semantic-conventions": "^1.25.1"
  }
}
